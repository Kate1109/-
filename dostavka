weight_robots = str(input())
limit_platforms = int(input())

weight_robots_list = list(map(int, weight_robots.split()))

def find_quantity_platforms(weight_robots_list, limit_platforms):
    platforma = 0
    left_pointer = 0
    right_pointer = len(weight_robots_list) - 1
    weight_robots_list.sort()
    while left_pointer <= right_pointer: 
        result = weight_robots_list[left_pointer] + weight_robots_list[right_pointer]
        if result <= limit_platforms:
            platforma += 1
            left_pointer += 1
            right_pointer -= 1
        else: 
            if weight_robots_list[right_pointer] >= weight_robots_list[left_pointer]:
                platforma += 1
                right_pointer -= 1
            if weight_robots_list[left_pointer] > weight_robots_list[right_pointer]:
                platforma += 1
                left_pointer += 1
            
    return platforma


print(find_quantity_platforms(weight_robots_list, limit_platforms))
